<script>

  class Hello {
    constructor(name) {
      this.name = name;
    }

    goodMorning() {
      alert("Good Morning " + this.name)
    }

    goodEvening() {
      alert("Good Evening " + this.name)
    }

  }

  const hello = new Hello("Taro");
  hello.goodMorning();
  hello.goodEvening();

</script>

